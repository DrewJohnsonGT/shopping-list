generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator zod {
  provider           = "zod-prisma-types"
  createPartialTypes = true
}

model Item {
  id          Int      @id @default(autoincrement())
  name        String
  description String
  quantity    Int
  checked     Boolean?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
